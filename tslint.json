{
    "defaultSeverity": "error",
    "extends": [
      "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
      "variable-name": [
        true,
        "ban-keywords",
        "check-format"
      ],
      "interface-name": {
        "options": [
          "never-prefix"
        ]
      },
      "quotemark": [true, "single", "jsx-single"],
      "no-console": [true, "log"],
      "object-literal-sort-keys": false,
      "ordered-imports": false,
      "member-ordering": false
    },
    "rulesDirectory": [],
    "linterOptions": {
      "exclude": [
        "node_modules/**/*",
        "build/**/*",
        "public/**/*"
      ]
    }
  }